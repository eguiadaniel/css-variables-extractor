{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["DOM", "DOM.Iterable", "es2019"],
    "module": "None",
    "strict": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["chrome"],
    "removeComments": true,
    "sourceMap": false,
    "isolatedModules": false
  },
  "include": ["src/**/*", "src/**/*.d.ts"],
  "exclude": ["node_modules"]
}
