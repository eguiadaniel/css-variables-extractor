{
  "name": "css-variables-extractor",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && npm run copy-files",
    "watch": "tsc -w",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css public/**/* dist"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.272",
    "copyfiles": "^2.4.1",
    "typescript": "^5.6.2"
  }
}
